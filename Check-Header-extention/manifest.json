{
  "manifest_version": 3,
  "name": "Security Header Checker",
  "version": "1.3",
  "description": "Cheack HTTP Security Headers follow OWASP all 17 header ",
  "permissions": [
    "webRequest", "webRequestBlocking", "tabs", "activeTab", "storage", "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
